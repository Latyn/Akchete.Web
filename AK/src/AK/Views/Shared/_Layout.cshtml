@{
    ViewBag.Title = "Home Page";
}
@section Scripts{
    <script src="~/wwwroot/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/wwwroot/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

}
<!DOCTYPE html>
<html>
<head>
    <title>Akchete - @ViewBag.Title </title>
    <meta charset="utf-8" />

    <link href="~/wwwroot/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/wwwroot/lib/owl-carousel/owl-carousel/owl.theme.css" rel="stylesheet" />
    <link href="~/wwwroot/lib/owl-carousel/owl-carousel/owl.carousel.css" rel="stylesheet" />
    <link href="~/wwwroot/lib/owl-carousel/owl-carousel/owl.transitions.css" rel="stylesheet" />
    <link href="~/wwwroot/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/wwwroot/lib/bootswatch/yeti/bootstrap.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body >
    <div class="row">
        <div class="navbar navbar-default menu" id="main_menu">
            <div class="col-md-1">
                <a asp-controller="App" asp-action="Index"><img src="~/img/Header6.png" alt="Ak7" class="headshot" /></a>
            </div>
            <div  class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                <div id="navbar" class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li><a asp-controller="App" asp-action="Index">Home</a></li>
                        <li><a asp-controller="App" asp-action="Profile">My Ak profile</a></li>
                        <li><a asp-controller="Event" asp-action="ListEvents"> Events</a></li>
                        <li><a asp-controller="App" asp-action="Models">Models</a></li>
                        <li><a asp-controller="App" asp-action="About">About</a></li>
                        <li><a asp-controller="App" asp-action="Contact">Contact</a></li>

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                @*<li><a asp-controllers="App" asp-action="Profile">My Ak profile</a></li>
                                <li><a asp-controllers="Event" asp-action="Events">Events</a></li>
                                <li><a asp-controllers="App" asp-action="Models">Models</a></li>
                                <li><a asp-controllers="App" asp-action="About">About</a></li>
                                <li><a asp-controllers="App" asp-action="Contact">Contact</a></li>
                                <li role="separator" class="divider"></li>
                                <li class="dropdown-header">Nav header</li>
                                <li><a href="#">Separated link</a></li>
                                <li><a href="#">One more separated link</a></li>*@
                            </ul>
                        </li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                        <li class="active"><a href="./">Default <span class="sr-only">(current)</span></a></li>
                        <li><a href="../navbar-static-top/">Static top</a></li>
                        <li><a href="../navbar-fixed-top/">Fixed top</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
             <div class="container">
            @RenderBody()
            </div>
        </div>

    <script type="text/javascript" src="~/wwwroot/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/map.js"> </script>
    <script type="text/javascript" src="~/js/site.js"></script>
    <script type="text/javascript" src="~/wwwroot/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script type="text/javascript" src="~/wwwroot/lib/owl-carousel/owl-carousel/owl.carousel.js"></script>

    @RenderSection("Scripts", false)

</body>
</html>